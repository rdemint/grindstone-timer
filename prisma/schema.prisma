// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Hangboard {
  id    Int    @id @default(autoincrement())
  name  String
  title String
}

model Interval {
  id                   Int       @id @default(autoincrement())
  workInterval         Int
  restInterval         Int
  leftHold             String
  leftHoldHangboard    String  
  rightHold            String
  rightHoldHangboard   String
  action               String
}
